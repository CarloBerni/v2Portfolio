-font-smoothing: antialiased;\n  -webkit-text-size-adjust: 100%;\n}\n\n// === Codepen Setup =====\nhtml,\nbody {\n  width: 100vw;\n  height: 100vh; \n}\n\n\nbody {\n  @extend %font-settings;\n  transform-style: preserve-3d;\n  transform: perspective($perspective);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-shadow: -0.035em 0 0 #F22613,  0.035em 0 0 #00FFFF;\n  overflow: hidden;\n}\n\n// === Menu <ul> =====\n.Menu-list {\n  font-size: $font-size;\n  font-weight: 800;\n  line-height: 1.2;\n  text-transform: uppercase;\n  text-align: center;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  transform: rotateX(-10deg) rotateY(20deg); // overwritten by JS\n  a {\n    text-decoration: none;\n  }\n}\n\n// === Menu item =====\n.Menu-list-item {\n  position: relative;\n  color: transparent;\n  cursor: pointer;\n  \n  // === Split Line =====\n  &::before {\n    content: '';\n    display: block;\n    position: absolute;\n    top: $split-position;\n    left: -10%;\n    right: -10%;\n    height: $split-thickness;\n    border-radius: $split-thickness;\n    margin-top: -($split-thickness / 2);\n    background: black;\n    transform: scale(0);\n    transition: transform .8s cubic-bezier(.16,1.08,.38,.98);\n    z-index: 1;\n  }\n}\n\n// === Top/bottom mask =====\n.Mask {\n  display: block;\n  position: absolute;\n  overflow: hidden;\n  color: $split-color;\n  top: 0;\n  height: $split-position;\n  transition: all .8s cubic-bezier(.16,1.08,.38,.98);\n  \n  span { display: block; }\n}\n\n// === Bottom mask specific =====\n.Mask + .Mask {\n  top: $split-position - 0.1;\n  height: 100 - $split-position + 0.1;\n  \n  span { transform: translateY(-$split-position); }\n}\n\n// === Where the fun stuff happens =====\n.Menu-list-item:hover,\n.Menu-list-item:active {\n  .Mask { color: #FFF; transform: skewX(12deg) translateX(5px); }\n  .Mask + .Mask { transform: skewX(12deg) translateX(-5px); }\n  &::before { transform: scale(1); }\n}\n\n#vantajs {\n  width: 100% !important;\n  height: 100% !important;\n  canvas{\n    position:initial !important;\n    width: 100% !important;\n    height: 100% !important;\n  }\n}\n\n.Menu{\n  position:absolute;\n}\n\n","scss/config/fonts.scss":"@font-face {\n  font-family: \"Kanit\";\n  font-style: normal;\n  font-weight: 200;\n  src:\n    url(\"../fonts/kanit-v4-latin-200.woff2\") format(\"woff2\"),\n    url(\"../fonts/kanit-v4-latin-200.woff\") format(\"woff\");\n}\n@font-face {\n  font-family: \"Kanit\";\n  font-style: normal;\n  font-weight: 400;\n  src: \n    url(\"../fonts/kanit-v4-latin-regular.woff2\") format(\"woff2\"),\n    url(\"../fonts/kanit-v4-latin-regular.woff\") format(\"woff\");\n}\n@font-face {\n  font-family: \"Kanit\";\n  font-style: normal;\n  font-weight: 800;\n  src: \n    url(\"../fonts/kanit-v4-latin-800.woff2\") format(\"woff2\"),\n    url(\"../fonts/kanit-v4-latin-800.woff\") format(\"woff\");\n}\n\n"},"lineCount":null}},"error":null,"hash":"c93af95fed7c042c3c3bbaf9bde62ded","cacheData":{"env":{}}}