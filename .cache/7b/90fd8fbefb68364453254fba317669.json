ch(function() {\n    var $this = $(this),\n        offsetLayer = $this.data('offset') || 0,\n        transformLayer = 'translate3d(' + offsetX * offsetLayer + 'px, ' + offsetY * offsetLayer + 'px, 20px)';\n\n    $this.css('transform', transformLayer);\n  });\n});\n\n\n\n\n// adding the plug\n\n\nVANTA.WAVES({\n  el: \"#vantajs\",\n  color: 0x0,\n  shininess: 150.00,\n  waveSpeed: 0.50,\n  zoom: 0.81\n})\n\n"},"lineCount":null}},"error":null,"hash":"f34c8baa6a2dc658eaa159a130419d66","cacheData":{"env":{}}}